# Allows loading FFTW3 settings from another project
set(FFTW3_CONFIG_FILE "${CMAKE_CURRENT_LIST_FILE}")

set(FFTW3_LIBRARIES fftw3)
set(FFTW3_LIBRARY_DIRS @CMAKE_INSTALL_FULL_LIBDIR@)
set(FFTW3_INCLUDE_DIRS @CMAKE_INSTALL_FULL_INCLUDEDIR@)

set(FFTW3f_LIBRARIES fftw3f)
set(FFTW3f_LIBRARY_DIRS @CMAKE_INSTALL_FULL_LIBDIR@)
set(FFTW3f_INCLUDE_DIRS @CMAKE_INSTALL_FULL_INCLUDEDIR@)

include("${CMAKE_CURRENT_LIST_DIR}/FFTW3LibraryDepends.cmake")
